        "node": "12.16.0",
        "npm": "6.13.7"
    },
    "private": true,
    "author": "soaple",
    "scripts": {
        "watch": "cross-env NODE_ENV=development npm run watch-app-dev",
        "watch-app-dev": "WEBPACK_DEV_SERVER_MODE=true webpack-dev-server --progress --config=webpack.config.js",
